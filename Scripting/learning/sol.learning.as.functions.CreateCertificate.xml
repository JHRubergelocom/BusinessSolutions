<rule>
  <name>Regel1</name>
  <condition></condition>
  <script>
    sol.common.as.Utils.requiresUserSession(this);

    var createCertificate = sol.create("sol.learning.as.functions.CreateCertificate", JSON.parse(EM_PARAM2));
    ruleset.setStatusMessage(JSON.stringify(createCertificate.execute()));

    sol.common.as.Utils.cleanupUserSession(this);
  </script>
</rule>
<rule>
  <name>Global Error Rule</name>
  <condition>OnError</condition>
