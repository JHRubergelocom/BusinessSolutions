<ruleset>
  <base>
    <name>sol.learning.LearningMonitoring</name>
    <search>
      <name>"DIRECT"</name>
      <value>"1"</value>
      <mask>0</mask>
      <max>200</max>
    </search>
    <interval>8H</interval>
    <onstart></onstart>
    <onend></onend>
    <includes>
      <name>lib_Class</name>
      <name>lib_moment</name>
      <name>lib_handlebars</name>
      <name>lib_sol.common.Cache</name>
      <name>lib_sol.common.Config</name>
      <name>lib_sol.common.Injection</name>
      <name>lib_sol.learning.mixins.Configuration</name>
      <name>lib_sol.common.ObjectUtils</name>
      <name>lib_sol.common.JsonUtils</name>
      <name>lib_sol.common.SordUtils</name>
      <name>lib_sol.common.DateUtils</name>
      <name>lib_sol.common.RepoUtils</name>
      <name>lib_sol.common.ExceptionUtils</name>
      <name>lib_sol.common.ObjectFormatter</name>
      <name>lib_sol.common.TranslateTerms</name>
      <name>lib_sol.common.Template</name>
      <name>lib_sol.common_monitoring.as.MonitorUtils</name>
      <name>lib_sol.common_monitoring.as.collectors.ChildrenCollector</name>
      <name>lib_sol.common_monitoring.as.collectors.NextRunCollector</name>
      <name>lib_sol.common_monitoring.as.analyzers.RuleAnalyzer</name>
      <name>lib_sol.common_monitoring.as.executors.SimpleExecutor</name>
      <name>lib_sol.common_monitoring.as.Monitor</name>
      <name>lib_sol.learning.as.LearningMonitoring</name>
    </includes>
  </base>
<rule>
  <name>Monitoring</name>
  <condition></condition>
  <script>
    EM_WRITE_CHANGED = false;
    var monitor = sol.create("sol.learning.as.LearningMonitoring");
    monitor.process();
  </script>
</rule>
<rule>
  <name>Global Error Rule</name>
  <condition>OnError</condition>
  <script>
    
  </script>
</rule>
</ruleset>
