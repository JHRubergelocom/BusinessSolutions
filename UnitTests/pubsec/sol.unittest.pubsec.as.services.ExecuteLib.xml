      <name>lib_sol.pubsec.as.ImportFilingPlan</name>
      <name>lib_sol.pubsec.as.actions.CreateDocument</name>
      <name>lib_sol.pubsec.as.actions.CreateFileCoverSheet</name>
      <name>lib_sol.pubsec.as.actions.CreateFileReport</name>
      <name>lib_sol.pubsec.as.actions.CreateFilesReport</name>
      <name>lib_sol.pubsec.as.actions.CreateFilingplanReport</name>
      <name>lib_sol.pubsec.as.actions.CreateLabel</name>
      <name>lib_sol.unittest.pubsec.as.services.ExecuteLib</name>
    </includes>
    <onstart>EM_SYS_SELECTOR = SordC.mbAll;</onstart>
  </base>
  <rule>
    <name>Regel1</name>
    <condition></condition>
    <script>
    sol.common.as.Utils.requiresUserSession(this);
    sol.common.as.Utils.executeFunction("sol.unittest.pubsec.as.services.ExecuteLib");
    sol.common.as.Utils.cleanupUserSession(this);
    </script>
  </rule>
  <rule>
    <name>Global Error Rule</name>
    <condition>OnError</condition>
    <script>sol.common.ExceptionUtils.logAsException(EM_ERROR);
    </script>
  </rule>
</ruleset>
